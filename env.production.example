# ðŸš€ ORPHI CrowdFund - Production Environment Variables Example
# Copy this file to .env.production and update with your values

# Application
NODE_ENV=production
VITE_APP_NAME=ORPHI CrowdFund
VITE_APP_DESCRIPTION=Web3 Crowdfunding Platform by LEAD 5
VITE_APP_VERSION=1.0.0

# Blockchain Configuration
VITE_NETWORK_ID=56
VITE_NETWORK_NAME=BSC Mainnet
VITE_RPC_URL=https://bsc-dataseed.binance.org/
VITE_BLOCK_EXPLORER=https://bscscan.com
VITE_CHAIN_ID=56

# Contract Configuration (Replace with your actual contract address)
VITE_CONTRACT_ADDRESS=0xYourContractAddressHere
VITE_CONTRACT_ABI_URL=/contract-abi.json

# API Configuration
VITE_API_BASE_URL=https://api.your-domain.com
VITE_WEBSOCKET_URL=wss://ws.your-domain.com

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Social Media & Branding
VITE_COMPANY_NAME=LEAD 5
VITE_SUPPORT_EMAIL=support@orphi.com
VITE_TELEGRAM_URL=https://t.me/orphicrowdfund
VITE_TWITTER_URL=https://twitter.com/orphicrowdfund

# Security
VITE_ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com
VITE_CSP_ENABLED=true

# Performance
VITE_ENABLE_PWA=true
VITE_CACHE_STRATEGY=cacheFirst

# Third-party Services (Optional)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_SENTRY_DSN=https://your-sentry-dsn
# VITE_HOTJAR_ID=your-hotjar-id 