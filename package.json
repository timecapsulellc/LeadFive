{
  "name": "leadfive",
  "version": "1.0.0",
  "description": "LeadFive - The Decentralized Incentive Platform",
  "type": "module",
  "main": "src/main.jsx",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 8080",
    "start": "npx serve -s dist -l 8080",
    "serve": "vite preview --host 0.0.0.0 --port 8080",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "deploy:testnet": "hardhat run scripts/deploy-leadfive-testnet.cjs --network bscTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy-leadfive-correct.cjs --network bsc",
    "deploy:correct": "hardhat run scripts/deploy-leadfive-correct.cjs --network bsc",
    "check:deployment": "hardhat run scripts/check-deployment.cjs --network bsc",
    "verify": "hardhat verify --network bsc",
    "verify:simple": "hardhat run scripts/verify-simple.cjs --network bsc",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix"
  },
  "keywords": [
    "decentralized-platform",
    "blockchain",
    "bsc",
    "defi",
    "network-incentives",
    "leadfive",
    "smart-contract",
    "react",
    "vite"
  ],
  "author": "LeadFive Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timecapsulellc/LeadFive.git"
  },
  "bugs": {
    "url": "https://github.com/timecapsulellc/LeadFive/issues"
  },
  "homepage": "https://leadfive.today",
  "dependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "ethers": "^6.8.1",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-d3-tree": "^3.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "terser": "^5.43.1",
    "vite": "^4.4.5",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.9",
    "@nomicfoundation/hardhat-verify": "^2.0.14",
    "@openzeppelin/contracts": "^5.3.0",
    "@openzeppelin/contracts-upgradeable": "^5.3.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.0",
    "dotenv": "^16.5.0",
    "hardhat": "^2.24.3",
    "hardhat-contract-sizer": "^2.10.0"
  },
  "engines": {
    "node": "20.17.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
